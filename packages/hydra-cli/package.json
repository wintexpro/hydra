{
  "name": "@subsquid/hydra-cli",
  "description": "CLI tool for building a Hydra query node",
  "version": "5.0.0-alpha.10",
  "author": "metmirr @metmirr subsquid @dzhelezov",
  "license": "MIT",
  "bin": {
    "hydra-cli": "./bin/run"
  },
  "engines": {
    "node": ">=14.14.0"
  },
  "files": [
    "/bin",
    "/lib",
    "!/lib/test",
    "/resource",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "hydra-cli",
    "scope": "@subsquid",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/errors"
    ]
  },
  "repository": "git@github.com:subsquid/hydra.git",
  "bugs": "https://github.com/subsquid/hydra/issues",
  "homepage": "https://www.subsquid.io/",
  "keywords": [
    "oclif"
  ],
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest",
    "readme": "oclif-dev readme",
    "test": "nyc mocha --forbid-only \"lib/test/**/*.test.js\""
  },
  "dependencies": {
    "@inquirer/input": "^0.0.13-alpha.0",
    "@inquirer/password": "^0.0.12-alpha.0",
    "@inquirer/select": "^0.0.13-alpha.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^2",
    "@octokit/auth-oauth-device": "^3.1.2",
    "@subsquid/openreader": "^0.2.0",
    "chalk": "^4.1.2",
    "cli-select": "^1.1.2",
    "cli-ux": "^5.6.3",
    "dotenv": "^10.0.0",
    "execa": "^4.1.0",
    "node-fetch": "^2.6.1",
    "pgtools": "^0.3.2",
    "simple-git": "^2.44.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@polkadot/types": "^6.4.2",
    "@types/inflected": "^1.1.29",
    "@types/nock": "^11.1.0",
    "nock": "^13.1.3",
    "typeorm": "0.2.38",
    "typescript": "~4.4.2"
  }
}
